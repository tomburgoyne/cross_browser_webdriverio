{
  "name": "cross_browser_webdriverio",
  "version": "1.0.0",
  "description": "Cross browser testing framework using WebdriverIO",
  "author": "Tom Burgoyne",
  "homepage": "https://github.com/tomburgoyne/cross_browser_webdriverio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomburgoyne/cross_browser_webdriverio.git"
  },
  "bugs": {
    "url": "https://github.com/tomburgoyne/cross_browser_webdriverio/issues"
  },
  "main": "./node_modules/.bin/wdio",
  "scripts": {
    "test:all": "wdio ./browserConfig/wdio.all.conf.js",
    "test:chrome": "wdio ./browserConfig/wdio.chrome.conf.js",
    "test:firefox": "wdio ./browserConfig/wdio.firefox.conf.js",
    "test:ie11": "wdio ./browserConfig/wdio.ie11.conf.js",
    "test:edge": "wdio ./browserConfig/wdio.edge.conf.js"
  },
  "keywords": [
    "test automation",
    "testing",
    "automation",
    "cross browser",
    "selenium",
    "webdriver",
    "webdriverio",
    "webdriverio v6",
    "mocha",
    "javascript"
  ],
  "engines": {
    "node": ">= 12.0"
  },
  "devDependencies": {
    "@wdio/cli": "6.4.0",
    "@wdio/local-runner": "6.4.0",
    "@wdio/mocha-framework": "6.4.0",
    "@wdio/selenium-standalone-service": "6.4.0",
    "@wdio/spec-reporter": "6.4.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-wdio": "6.0.12",
    "prettier": "2.0.5"
  }
}
